{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:seed": "yarn prisma db seed",
    "prisma:studio": "yarn prisma studio",
    "prisma:dev": "yarn docker:up && dotenv -e .env yarn prisma migrate dev",
    "prisma:reset": "yarn docker:up && dotenv -e .env yarn prisma migrate reset",
    "prisma:test": "yarn docker:up && dotenv -e .env.test yarn prisma migrate dev",
    "start": "node .",
    "dev": "yarn prisma:dev && nodemon .",
    "prettier": "prettier --write \"{prisma,src,tests}/**/*.js\" --config \".prettierrc\"",
    "test": "yarn prisma:test && cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "accesscontrol": "^2.2.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "ical-generator": "^3.4.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.8",
    "rrule": "^2.7.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/node": "^18.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.1.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
